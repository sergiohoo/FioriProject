{
  "_version": "1.12.0",
  "sap.app": {
    "id": "logaligroup.Employees",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "ach": "ach",
    "sourceTemplate": {
      "id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
      "version": "1.40.12"
    },
    "dataSources": {
      "Northwind.svc": {
        "uri": "Northwind/v2/Northwind/Northwind.svc/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadata.xml"
        }
      },
      "YSAPUI5_SRV_01": {
        "uri": "/sap/opu/odata/sap/YSAPUI5_SRV_01/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/YSAPUI5_SRV_01/metadata.xml"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "rootView": {
      "viewName": "logaligroup.Employees.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.60.1",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "logaligroup.Employees.i18n.i18n"
        }
      },
      "odataNorthwind": {
        "dataSource": "Northwind.svc",
        "preload": true,
        "settings": {
          "defaultBindingMode": "OneWay",
          "defaultOperationMode" : "Server",
          "defaultCountMode": "Request",
          "refreshAfterChange": false
        }
      },
      "incidenceModel": {
        "type": "sap.ui.model.odata.v2.ODataModel",
        "dataSource": "YSAPUI5_SRV_01",
        "preload": true,
        "settings": {
          "defaultBindingMode": "TwoWay",
          "defaultOperationMode" : "Server",
          "defaultCountMode": "Request",
          "refreshAfterChange": false,
          "useBatch": false,
          "defaultUpdateMethod": "Put"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "logaligroup.Employees.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": "",
          "target": [
            "TargetMain"
          ]
        },
        {
          "name": "RouteOrderDetails",
          "pattern": "OrderDetails/{OrderID}",
          "target": [
            "TargetOrderDetails"
          ]
        }
      ],
      "targets": {
        "TargetMain": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Main",
          "viewName": "Main"
        },
        "TargetOrderDetails": {
          "viewType": "XML",
          "viewName": "OrderDetails"
        }
      }
    }
  }
}